{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["jquery", "eslint:recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "script",
        "parser": "babel"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "prefer-const": "error",
        "no-var": "error",
        "lines-around-comment": "off",
        "space-before-function-paren": "off",
        "no-undef": "warn",
        "no-unused-expressions": "warn",
        "eqeqeq": "error",
        "yoda": "error",
        "space-in-parens": "off"
    }
}