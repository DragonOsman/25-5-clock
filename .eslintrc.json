/* eslint-disable no-undef */
{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "eslint:recommended",
      "standard",
      "jquery"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2020,
      "parser": "babel"
  },
  "rules": {
      "eqeqeq": "error",
      "quotes": [
          "error",
          "double"
      ],
      "prefer-const": "error",
      "no-var": "error",
      "newline-before-return": "off",
      "semi": [
        2, 
        "always"
      ],
      "no-extra-semi": "error",
      "space-before-function-paren": "off",
      "lines-around-comment": "warn",
      "no-undef": "warn",
      "no-unused-expressions": "warn"
  }
}